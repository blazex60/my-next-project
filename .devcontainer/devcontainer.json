{
    "name": "my-next-project-devcontainer",
    "dockerComposeFile":"docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/my-next-project",
    "remoteUser": "node",
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint"
            ],
            "settings": {
                "files.autoSave": "afterDelay",
                "editor.formatOnSave": true
            }
        }
    }
}